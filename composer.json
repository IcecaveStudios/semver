{
    "name": "icecave/semver",
    "type": "library",
    "description": "Semantic version parser and comparators for PHP.",
    "keywords": ["semantic", "version", "semver"],
    "homepage": "https://github.com/IcecaveStudios/semver",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "icecave/testing": "dev-master as 1.1.3",
        "eloquent/typhoon": "0.6.*"
    },
    "autoload": {
        "psr-0": {
            "Icecave\\SemVer": "lib",
            "Icecave\\SemVer\\TypeCheck": "lib-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "lib-typhoon",
            "validator-namespace": "Icecave\\SemVer\\TypeCheck",
            "use-native-callable": false
        }
    }
}
