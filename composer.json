{
    "name": "icecave/semver",
    "description": "A semantic version parser and comparison library.",
    "keywords": ["semantic", "version", "semver", "compare", "comparison"],
    "homepage": "https://github.com/IcecaveStudios/semver",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.3",
        "icecave/parity": "~0.1.0"
    },
    "require-dev": {
        "eloquent/typhoon": "~0.9.0",
        "icecave/archer": "~1"
    },
    "suggest": {
        "eloquent/asplode": "Drop-in exception-based error handling."
    },
    "autoload": {
        "psr-0": {
            "Icecave\\SemVer": "src",
            "Icecave\\SemVer\\TypeCheck": "src-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "src-typhoon",
            "validator-namespace": "Icecave\\SemVer\\TypeCheck",
            "use-native-callable": false
        }
    }
}
